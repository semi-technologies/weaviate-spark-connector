sc.setLogLevel("info")
case class Article (title: String, content: String)
val articles = Seq( Article("Sam", "Sam"))
val ds = articles.toDF

ds.write
  .format("io.weaviate.spark.Weaviate")
  .option("scheme", "http")
  .option("host", "localhost:8080")
  .option("className", "Article")
  .mode("append")
  .save()