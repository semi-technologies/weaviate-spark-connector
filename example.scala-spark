sc.setLogLevel("info")
case class Article (title: String, content: String, wordCount: Int)
val articles = Seq( Article("Sam", "Sam and Sam", 3)).toDF

articles.write
  .format("io.weaviate.spark.Weaviate")
  .option("scheme", "http")
  .option("host", "localhost:8080")
  .option("className", "Article")
  .mode("append")
  .save()